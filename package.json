{
  "name": "customasm",
  "displayName": "customasm",
  "author": {
    "name": "JosephAbbey",
    "email": "joseph@abbey1.org.uk",
    "url": "https://joseph.abbey1.org.uk"
  },
  "publisher": "JosephAbbey",
  "description": "",
  "version": "1.0.4",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:customasm.compile"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/josephabbey/customasm-vscode"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "customasm.compile",
        "title": "Compile with Customasm",
        "category": "Assembly"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "customasm.compile",
          "when": "editorLangId == asm || editorLangId == asm-intel-x86-generic",
          "group": "navigation"
        }
      ]
    },
    "languages": [
      {
        "id": "asm-intel-x86-generic",
        "extensions": [
          ".asm"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
